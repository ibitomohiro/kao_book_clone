<h3> Confirm before post </h3>
<%= form_with(model:@post, url:posts_path, local: true) do |f| %>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <div class="confirm-img">
        <%= image_tag(@post.image.url, size: '200x200') if @post.image.url %>
      </div>
      <%= f.hidden_field :image_cache %>
      <%= f.hidden_field :content %>
      <%= f.submit "Post", class: "btn btn-primary" %>
    </div>
   </div>
<% end %>

<%= form_with(model:@post, url:new_post_path, local:true, method: "get") do |f| %>
  <div class="row">
     <div class="col-md-6 col-md-offset-3">
      <%= f.hidden_field :iamge_cache %>
      <%= f.submit "戻る", name: "back", class: "btn btn-danger"%>
    </div>
  </div>
<% end %>