<h1>新しく投稿する</h1>
<div class="content">
<%= form_with(model: Post.new, local: true) do |f| %>
 <div class="post_title">
  <%= f.label :title %>
  <%= f.text_field :title %>
 </div>
 <div class="post_content">
  <%= f.label :content %>
  <%= f.text_area :content %>
 </div>
 <div class="post_image">
  <%= f.label :image %>
  <%= f.file_field :image %>
 </div>
 <%= f.submit %>
 <% end %>
</div>